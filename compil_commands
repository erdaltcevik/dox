                                                                                        android common linux

cd /dvl       
git clone https://android.googlesource.com/kernel/common
cd /dvl/common                                                     # alias cm="cd /dvl/common && l"
git checkout android-4.9
cd /dvl/common
source /dvl/common/kernel/configs/android-fetch-configs.sh
cp /dvl/common/arch/arm/configs/sunxi_defconfig /tks/common/arch/arm/configs/mb734.cfg

cp arch/arm/boot/dts/sun8i-a33-inet-d978-rev2.dts /tks/common/arch/arm/boot/dts/sun8i-a33-mb734.dts
ln -s /tks/common/arch/arm/boot/dts/sun8i-a33-mb734.dts arch/arm/boot/dts/sun8i-a33-mb734.dts

make -j 8 ARCH=arm CROSS_COMPILE=arm-linux-androideabi-                   distclean
make -j 8 ARCH=arm CROSS_COMPILE=arm-linux-androideabi- O=/tks/common_out distclean
rm /tks/common_out/usr/*

export KCONFIG_CONFIG=/tks/common_out/.config
ARCH=arm CROSS_COMPILE=arm-linux-androideabi- ./scripts/kconfig/merge_config.sh -m -r   \
/tks/common/arch/arm/configs/mb734.cfg                                                  \
kernel/configs/android-base.cfg                                                         \
kernel/configs/android-recommended.cfg

make -j 8 ARCH=arm CROSS_COMPILE=arm-linux-androideabi- O=/tks/common_out menuconfig
make -j 8 ARCH=arm CROSS_COMPILE=arm-linux-androideabi- O=/tks/common_out zImage
make -j 8 ARCH=arm CROSS_COMPILE=arm-linux-androideabi- O=/tks/common_out sun8i-a33-mb734.dtb

sudo dd of=/dev/sde if=/tks/common_out/arch/arm/boot/zImage                  bs=512 seek=2048  conv=sync status=progress ; sync
sudo dd of=/dev/sde if=/tks/common_out/arch/arm/boot/dts/sun8i-a33-mb734.dtb bs=512 seek=18432 conv=sync status=progress ; sync





