mkdir /dvl/aosp
cd /dvl/aosp
git config --global core.autocrlf input
git config --global user.name "Erdal T Cevik"                          // No non-English chars allowed
git config --global user.email "erdaltcevik@gmail.com"
repo init -u https://android.googlesource.com/platform/manifest -b android-7.1.2_r27
repo sync

export OUT_DIR_COMMON_BASE=/tks/aosp_out
make clobber
source build/envsetup.sh
choosecombo
   -debug
   -generic
   -eng
make -j 8

mkdir /tks/aosp_out/aosp/debug/target/product/generic/ramdisk
cd /tks/aosp_out/aosp/debug/target/product/generic/ramdisk
gunzip -c /tks/aosp_out/aosp/debug/target/product/generic/ramdisk.img | cpio -i



sudo mount -o loop /tks/aosp_out/aosp/debug/target/product/generic/system.img /mnt

