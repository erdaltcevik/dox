                                                                                                            u-boot-sunxi
cd /dvl
git clone git://git.denx.de/u-boot-sunxi.git <path_to_dir>
git checkout v2017.11

#git remote add u-boot git://git.denx.de/u-boot.git
#git fetch u-boot
#git checkout -b v2017.11 u-boot/master

cd /dvl/u-boot-sunxi
cp /dvl/u-boot-sunxi/configs/q8_a33_tablet_1024x600_defconfig /tks/u-boot-sunxi/configs/mb734_defconfig
ln -s /tks/u-boot-sunxi/configs/mb734_defconfig /dvl/u-boot-sunxi/configs/mb734_defconfig
cp /dvl/u-boot-sunxi/arch/arm/dts/sun8i-a33-inet-d978-rev2.dts /tks/u-boot-sunxi/arch/arm/dts/sun8i-a33-mb734.dts
ln -s /tks/u-boot-sunxi/arch/arm/dts/sun8i-a33-mb734.dts /dvl/u-boot-sunxi/arch/arm/dts/sun8i-a33-mb734.dts

cd /dvl/u-boot-sunxi
make -j 8 CROSS_COMPILE=arm-linux-gnueabihf- distclean
make -j 8 CROSS_COMPILE=arm-linux-gnueabihf- O=/tks/u-boot-sunxi_out distclean
make -j 8 CROSS_COMPILE=arm-linux-gnueabihf- O=/tks/u-boot-sunxi_out mb734_config
make -j 8 CROSS_COMPILE=arm-linux-gnueabihf- O=/tks/u-boot-sunxi_out menuconfig
make -j 8 CROSS_COMPILE=arm-linux-gnueabihf- O=/tks/u-boot-sunxi_out all

sudo dd of=/dev/sde if=/tks/u-boot-sunxi_out/spl/sunxi-spl.bin bs=512 seek=16 conv=sync status=progress ; sync
sudo dd of=/dev/sde if=/tks/u-boot-sunxi_out/u-boot.img        bs=512 seek=80 conv=sync status=progress ; sync
